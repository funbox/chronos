{
  "name": "@funboxteam/chronos",
  "version": "1.0.1",
  "description": "The library for ruling the time",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint --cache -c .eslintrc.js --ext .js lib",
    "build": "BABEL_ENV=esm babel lib --out-dir dist/esm && BABEL_ENV=node babel lib --out-dir dist/cjs",
    "prepublishOnly": "npm run build",
    "test": "BABEL_ENV=node mocha --require @babel/register tests"
  },
  "author": "Natalia Andreychenko <natandreychenko@gmail.com> (https://github.com/natrey)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/register": "7.6.2",
    "@funboxteam/eslint-config": "4.14.0",
    "chai": "4.2.0",
    "chai-datetime": "1.5.0",
    "eslint": "6.5.1",
    "husky": "3.0.8",
    "lint-staged": "9.4.1",
    "mocha": "6.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged -q"
    }
  },
  "lint-staged": {
    "lib/**/*.js": [
      "eslint --fix --cache -c .eslintrc.js",
      "git add"
    ]
  },
  "dependencies": {}
}
